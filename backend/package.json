{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "start": "tsc && node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "docker:setup": "docker-compose -f scripts/docker-compose.yml up --build -d",
    "docker:reset": "docker-compose -f scripts/docker-compose.yml down -v && npm run docker:setup",
    "database:reset": "node scripts/reset_database.js",
    "database:mock:sample": "node scripts/mock_database.js sample",
    "database:mock:hardcoded": "node scripts/mock_database.js hardcoded",
    "database:store": "node scripts/db_backup.js store",
    "database:restore": "npm run database:reset && node scripts/db_backup.js restore",
    "storage:setup": "node scripts/setup_storage.js --setup",
    "storage:reset": "node scripts/setup_storage.js --reset",
    "storage:clean": "node scripts/setup_storage.js --clean",
    "setup:clean": "npm run database:reset && npm run storage:clean",
    "setup:reset:hardcoded": "npm run docker:reset && npm run database:reset && npm run storage:reset && npm run database:mock:hardcoded",
    "lint": "eslint .",
    "lintFixAll": "eslint './**/*.{js,jsx,ts,tsx}'",
    "prettierFixAll": "prettier --write './**/*.{js,jsx,ts,tsx}'",
    "fix:lintPrettier": "npm run prettierFixAll && npm run lintFixAll",
    "env:refresh:staging": "rm .env || : && cp ../.env_staging .env",
    "env:refresh:prod": "rm .env || : && cp ../.env_prod .env",
    "env:refresh:local": "rm .env || : && cp ../.env_local .env"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.635.0",
    "@aws-sdk/s3-request-presigner": "^3.635.0",
    "@types/nodemailer": "^6.4.15",
    "@types/validator": "^13.12.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.14",
    "pg": "^8.12.0",
    "socket.io": "^4.7.5",
    "typescript": "^5.5.4",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/bcryptjs": "^2.4.6",
    "@types/pg": "^8.11.6",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@types/node": "^22.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-native": "^4.1.0",
    "globals": "^15.8.0",
    "prettier": "^3.3.2",
    "ts-node-dev": "^2.0.0",
    "typescript-eslint": "^7.15.0"
  }
}
