{
  "name": "wymianka",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "lintFixAll": "eslint 'app/**/*.{js,jsx,ts,tsx}'",
    "prettierFixAll": "prettier --write 'app/**/*.{js,jsx,ts,tsx}'",
    "fix:lintPrettier": "npm run prettierFixAll && npm run lintFixAll",
    "env:refresh:staging": "node scripts/refresh_env.js staging",
    "env:refresh:prod": "node scripts/refresh_env.js prod",
    "env:refresh:local": "node scripts/refresh_env.js local",
    "eas:build:local:android": "eas build --platform android --local",
    "eas:build:local:ios": "eas build --platform ios --local"
  },
  "dependencies": {
    "@react-native/metro-config": "^0.75.3",
    "@types/validator": "^13.12.0",
    "expo": "^51.0.32",
    "expo-asset": "~10.0.10",
    "expo-av": "~14.0.7",
    "expo-camera": "~15.0.14",
    "expo-clipboard": "~6.0.3",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.26",
    "expo-file-system": "~17.0.1",
    "expo-font": "~12.0.10",
    "expo-image-manipulator": "~12.0.5",
    "expo-image-picker": "~15.0.7",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-location": "~17.0.1",
    "expo-router": "~3.5.17",
    "expo-secure-store": "~13.0.2",
    "expo-status-bar": "~1.12.1",
    "fast-xml-parser": "^4.4.1",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-mime-types": "^2.5.0",
    "react-native-popup-menu": "^0.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-svg-transformer": "^1.5.0",
    "react-native-uuid": "^2.0.2",
    "socket.io-client": "^4.7.5",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@eslint/js": "^9.6.0",
    "@types/react": "~18.2.45",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-native": "^4.1.0",
    "globals": "^15.8.0",
    "prettier": "^3.3.2",
    "typescript": "~5.3.3",
    "typescript-eslint": "^7.15.0"
  },
  "private": true
}
